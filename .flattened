<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>no.nav.foreldrepenger.kontrakter</groupId>
  <artifactId>foreldrepenger-kontrakter-root</artifactId>
  <version>5.0-SNAPSHOT</version>
  <packaging>pom</packaging>
  <name>(FPSAK) Kontrakter - Root</name>
  <distributionManagement>
    <repository>
      <id>m2internal</id>
      <name>NAV Maven2 Internal Repository</name>
      <url>https://repo.adeo.no/nexus/content/repositories/m2internal</url>
    </repository>
    <snapshotRepository>
      <id>m2snapshot</id>
      <name>NAV Maven2 Snapshot Repository</name>
      <url>https://repo.adeo.no/nexus/content/repositories/m2snapshot</url>
    </snapshotRepository>
  </distributionManagement>
  <properties>
    <java.version>10</java.version>
    <maven.javadoc.skip>true</maven.javadoc.skip>
    <revision>5.0</revision>
    <sha1></sha1>
    <changelist>-SNAPSHOT</changelist>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>org.assertj</groupId>
        <artifactId>assertj-core</artifactId>
        <version>3.8.0</version>
      </dependency>
      <dependency>
        <groupId>junit</groupId>
        <artifactId>junit</artifactId>
        <version>4.12</version>
      </dependency>
      <dependency>
        <groupId>org.glassfish.jaxb</groupId>
        <artifactId>jaxb-runtime</artifactId>
        <version>2.4.0-b180830.0438</version>
      </dependency>
      <dependency>
        <groupId>javax.validation</groupId>
        <artifactId>validation-api</artifactId>
        <version>1.1.0.Final</version>
      </dependency>
      <dependency>
        <groupId>com.fasterxml.jackson</groupId>
        <artifactId>jackson-bom</artifactId>
        <version>2.9.7</version>
        <type>pom</type>
        <scope>import</scope>
      </dependency>
      <dependency>
        <groupId>no.nav.foreldrepenger.kontrakter</groupId>
        <artifactId>fp-feed-v1</artifactId>
        <version>${project.version}</version>
      </dependency>
      <dependency>
        <groupId>no.nav.foreldrepenger.kontrakter</groupId>
        <artifactId>inntektsmelding-v1</artifactId>
        <version>${project.version}</version>
      </dependency>
      <dependency>
        <groupId>no.nav.foreldrepenger.kontrakter</groupId>
        <artifactId>soeknad-v1</artifactId>
        <version>${project.version}</version>
      </dependency>
    </dependencies>
  </dependencyManagement>
</project>

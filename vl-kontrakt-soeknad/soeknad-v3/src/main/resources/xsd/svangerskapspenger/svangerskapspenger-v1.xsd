<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:felles="urn:no:nav:vedtak:felles:xml:soeknad:felles:v3" xmlns:sv="urn:no:nav:vedtak:felles:xml:soeknad:svangerskapspenger:v1" xmlns:kv="urn:no:nav:vedtak:felles:xml:soeknad:kodeverk:v3" targetNamespace="urn:no:nav:vedtak:felles:xml:soeknad:svangerskapspenger:v1" elementFormDefault="qualified">
	<xsd:import namespace="urn:no:nav:vedtak:felles:xml:soeknad:felles:v3" schemaLocation="../felles/felles-v3.xsd"/>
	<xsd:import namespace="urn:no:nav:vedtak:felles:xml:soeknad:kodeverk:v3" schemaLocation="../kodeverk/kodeverk-v3.xsd"/>
	<xsd:complexType name="Svangerskapspenger">
		<xsd:annotation>
			<xsd:documentation/>
		</xsd:annotation>
		<xsd:complexContent>
			<xsd:extension base="felles:Ytelse">
				<xsd:sequence>
					<xsd:element name="termindato" type="xsd:date"/>
					<xsd:element name="fødselsdato" type="xsd:date" minOccurs="0"/>
					<xsd:element name="medlemskap" type="sv:Medlemskap"/>
					<xsd:element name="opptjening" type="sv:Opptjening" minOccurs="0"/>
					<xsd:element name="tilretteleggingListe" type="sv:TilretteleggingListe"/>
				</xsd:sequence>
				<xsd:anyAttribute processContents="lax"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="Medlemskap">
		<xsd:sequence>
			<xsd:element name="iNorgeVedFødselstidspunkt" type="xsd:boolean"/>
			<xsd:element name="boddINorgeSiste12Mnd" type="xsd:boolean"/>
			<xsd:element name="borINorgeNeste12Mnd" type="xsd:boolean"/>
			<xsd:element name="oppholdUtland" type="sv:OppholdUtland" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:anyAttribute processContents="lax"/>
	</xsd:complexType>
	<xsd:complexType name="OppholdUtland">
		<xsd:sequence>
			<xsd:element name="periode" type="felles:Periode"/>
			<xsd:element name="land" type="kv:Land"/>
		</xsd:sequence>
		<xsd:anyAttribute processContents="lax"/>
	</xsd:complexType>
	<xsd:complexType name="Opptjening">
		<xsd:sequence>
			<xsd:element name="utenlandskArbeidsforhold" type="sv:UtenlandskArbeidsforhold" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="frilans" type="sv:Frilans" minOccurs="0"/>
			<xsd:element name="egenNæring" type="sv:EgenNæring" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="annenOpptjening" type="sv:AnnenOpptjening" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="UtenlandskArbeidsforhold">
		<xsd:sequence>
			<xsd:element name="periode" type="felles:Periode"/>
			<xsd:element name="arbeidsgiversnavn" type="xsd:string" minOccurs="0"/>
			<xsd:element name="arbeidsland" type="kv:Land"/>
			<xsd:element name="vedlegg" type="xsd:IDREF" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:anyAttribute processContents="lax"/>
	</xsd:complexType>
	<xsd:complexType name="Frilans">
		<xsd:sequence>
			<xsd:element name="periode" type="felles:Periode" maxOccurs="unbounded"/>
			<xsd:element name="harInntektFraFosterhjem" type="xsd:boolean"/>
			<xsd:element name="erNyoppstartet" type="xsd:boolean"/>
			<xsd:element name="nærRelasjon" type="xsd:boolean"/>
			<xsd:element name="frilansoppdrag" type="sv:Frilansoppdrag" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="vedlegg" type="xsd:IDREF" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:anyAttribute processContents="lax"/>
	</xsd:complexType>
	<xsd:complexType name="Frilansoppdrag">
		<xsd:sequence>
			<xsd:element name="periode" type="felles:Periode"/>
			<xsd:element name="oppdragsgiver" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:anyAttribute processContents="lax"/>
	</xsd:complexType>
	<xsd:complexType name="AnnenOpptjening">
		<xsd:sequence>
			<xsd:element name="type" type="kv:AnnenOpptjeningTyper"/>
			<xsd:element name="periode" type="felles:Periode"/>
			<xsd:element name="vedlegg" type="xsd:IDREF" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:anyAttribute processContents="lax"/>
	</xsd:complexType>
	<xsd:complexType name="EgenNæring">
		<xsd:sequence>
			<xsd:element name="virksomhetstype" type="kv:Virksomhetstyper" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="periode" type="felles:Periode"/>
			<xsd:element name="erNyoppstartet" type="xsd:boolean"/>
			<xsd:element name="erVarigEndring" type="xsd:boolean"/>
			<xsd:element name="erNyIArbeidslivet" type="xsd:boolean"/>
			<xsd:element name="nærRelasjon" type="xsd:boolean" minOccurs="0"/>
			<xsd:element name="næringsinntektBrutto" type="xsd:integer" minOccurs="0"/>
			<xsd:element name="oppstartsdato" type="xsd:date" minOccurs="0"/>
			<xsd:element name="endringsdato" type="xsd:date" minOccurs="0"/>
			<xsd:element name="beskrivelseAvEndring" type="xsd:string" minOccurs="0"/>
			<xsd:element name="regnskapsfører" type="sv:Regnskapsfører" minOccurs="0"/>
			<xsd:element name="vedlegg" type="xsd:IDREF" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:anyAttribute processContents="lax"/>
	</xsd:complexType>
	<xsd:complexType name="Regnskapsfører">
		<xsd:sequence>
			<xsd:element name="navn" type="xsd:string" minOccurs="0"/>
			<xsd:element name="telefon" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
		<xsd:anyAttribute processContents="lax"/>
	</xsd:complexType>
	<xsd:complexType name="TilretteleggingListe">
		<xsd:sequence>
			<xsd:element name="tilrettelegging" type="sv:Tilrettelegging" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:anyAttribute processContents="lax"/>
	</xsd:complexType>
	<xsd:complexType name="Tilrettelegging" abstract="true">
		<xsd:sequence>
			<xsd:element name="arbeidsforhold" type="sv:Arbeidsforhold"/>
			<xsd:element name="behovForTilretteleggingFom" type="xsd:date"/>
		</xsd:sequence>
		<xsd:anyAttribute processContents="lax"/>
	</xsd:complexType>
	<xsd:complexType name="HelTilrettelegging">
		<xsd:complexContent>
			<xsd:extension base="sv:Tilrettelegging">
				<xsd:sequence>
					<xsd:element name="tilrettelagtArbeidFom" type="xsd:date"/>
				</xsd:sequence>
				<xsd:anyAttribute processContents="lax"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="DelvisTilrettelegging">
		<xsd:complexContent>
			<xsd:extension base="sv:Tilrettelegging">
				<xsd:sequence>
					<xsd:element name="tilrettelagtArbeidFom" type="xsd:date"/>
					<xsd:element name="stillingsprosent" type="xsd:decimal"/>
				</xsd:sequence>
				<xsd:anyAttribute processContents="lax"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="IngenTilrettelegging">
		<xsd:complexContent>
			<xsd:extension base="sv:Tilrettelegging">
				<xsd:sequence>
					<xsd:element name="slutteArbeidFom" type="xsd:date"/>
				</xsd:sequence>
				<xsd:anyAttribute processContents="lax"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="Arbeidsforhold" abstract="true"/>
	<xsd:complexType name="Arbeidsgiver" abstract="true">
		<xsd:complexContent>
			<xsd:extension base="sv:Arbeidsforhold">
				<xsd:sequence>
					<xsd:element name="identifikator" type="xsd:string"/>
					<xsd:element name="vedlegg" type="xsd:IDREF" maxOccurs="unbounded"/>
				</xsd:sequence>
				<xsd:anyAttribute processContents="lax"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="Virksomhet">
		<xsd:complexContent>
			<xsd:extension base="sv:Arbeidsgiver">
				<xsd:anyAttribute processContents="lax"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="PrivatArbeidsgiver">
		<xsd:complexContent>
			<xsd:extension base="sv:Arbeidsgiver"/>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="Frilanser">
		<xsd:complexContent>
			<xsd:extension base="sv:Arbeidsforhold">
				<xsd:sequence>
					<xsd:element name="opplysningerOmRisikofaktorer" type="xsd:string"/>
					<xsd:element name="opplysningerOmTilretteleggingstiltak" type="xsd:string"/>
					<xsd:element name="vedlegg" type="xsd:IDREF" maxOccurs="unbounded"/>
				</xsd:sequence>
				<xsd:anyAttribute processContents="lax"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:complexType name="SelvstendigNæringsdrivende">
		<xsd:complexContent>
			<xsd:extension base="sv:Arbeidsforhold">
				<xsd:sequence>
					<xsd:element name="opplysningerOmRisikofaktorer" type="xsd:string"/>
					<xsd:element name="opplysningerOmTilretteleggingstiltak" type="xsd:string"/>
					<xsd:element name="vedlegg" type="xsd:IDREF" maxOccurs="unbounded"/>
				</xsd:sequence>
				<xsd:anyAttribute processContents="lax"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
	<xsd:element name="svangerskapspenger" type="sv:Svangerskapspenger"/>
</xsd:schema>
